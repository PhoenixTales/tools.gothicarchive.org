{
  "domain": "gothicarchive.org",
  "repositories": {
    "audio": {
      "fileTypes": [ "mp3", "ogg" ]
    },
    "bin": {
      "fileTypes": [ "*" ]
    },
    "images": {
      "fileTypes": [ "gif", "ico", "jpg", "jpeg", "png" ]
    },
    "videos": {
      "fileTypes": [ "avi", "gif", "mov", "mpg", "mp4", "wmv" ]
    },
    "www": {
      "fileTypes": [ "css", "js", "md", "txt", "html", "htm", "swf", "wasm" ],
      "specialFiles": [ "Gemfile", ".gitignore", ".gitmodules", "_config.yml" ]
    }
  },
  "standardFiles": [
    "CNAME",
    "favicon.ico",
    "index.html",
    "README.md"
  ]
}
